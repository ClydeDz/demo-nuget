<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Clyde D'Souza</Authors>
    <Company>Clyde D'Souza</Company>
    <PackageProjectUrl>https://github.com/ClydeDz/demo-nuget/</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/ClydeDz/demo-nuget/blob/master/LICENSE</PackageLicenseUrl>
    <Copyright>Copyright (c) 2019 Clyde D'Souza</Copyright>
    <Description>Some description for Demo Nuget</Description>
    <Product>Demo Nuget</Product>
    <PackageIconUrl>https://via.placeholder.com/150</PackageIconUrl>
    <RepositoryUrl>https://github.com/ClydeDz/demo-nuget/</RepositoryUrl>
    <PackageTags>Nuget, Demo</PackageTags>
    <PackageReleaseNotes>Sample release notes for demo nuget</PackageReleaseNotes>
    <PackageId>DemoNugetByClydeDSouza</PackageId>
    <AssemblyVersion>1.0.0.5</AssemblyVersion>
    <FileVersion>1.0.0.5</FileVersion>
    <AssemblyName>DemoNuget</AssemblyName>
    <RootNamespace>DemoNuget</RootNamespace>
    <Version>1.0.5</Version>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <ApplicationIcon />
    <Win32Resource />
  </PropertyGroup>

  <ItemGroup>
    <None Remove="license.txt" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="license\license.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
